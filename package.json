{
  "name": "coodev",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "repository": "https://github.com/wokeyi/coodev",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run clean & pnpm -C ./packages/core build & pnpm -C ./packages/react build & pnpm -C ./packages/test build",
    "clean": "rimraf ./packages/core/lib && rimraf ./packages/react/lib",
    "clean:all": "rimraf ./packages/core/node_modules && rimraf ./packages/react/node_modules && rimraf ./node_modules",
    "dev": "pnpm -C ./packages/core dev & pnpm -C ./packages/react dev"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
