{
  "name": "coodev",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "repository": "https://github.com/wokeyi/coodev",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -C ./packages/core build & pnpm -C ./packages/react build",
    "clean": "lerna exec -- rm -rf lib",
    "dev": "lerna run dev --scope @coodev/core --scope @coodev/react --scope @coodev/document --scope @coodev/webpack --parallel"
  },
  "devDependencies": {
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
